{
  "image": {
    "href": "https://watsonhubconnectors.ssdevrd.com/images/connector.png"
  },
  "object_types": {
    "card": {
      "doc": {
        "href": "https://github.com/vmwaresamples/card-connectors-guide/wiki/Card-Responses"
      },
      "fields": {
        "ticket_id": {
          "capture_group": 1,
          "regex": "\\b(REQ[0-9]{7})\\b"
        },
        "email": {
          "env": "USER_EMAIL"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/cards/requests"
      }
    },
    "catalogItem": {
      "fields": {
        "type": {
          "env": "ITEM_TYPE"
        },
        "catalog": {
          "env": "CATALOG_NAME"
        },
        "category": {
          "env": "CATEGORY_NAME"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/items/"
      }
    },
    "serviceRequest": {
      "fields": {
        "items": {
          "env": "ITEMS_LIST"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/items/"
      }
    },
    "ticketType": {
      "fields": {
        "ticketType": {
          "env": "TICKET_TYPE"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/tickets/{ticketType}/"
      }
    },
    "ticketDetails": {
      "fields": {
        "ticketType": {
          "env": "TICKET_TYPE"
        },
        "ticketNumber": {
          "env": "TICKET_NUMBER"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/ticket/{ticketType}/{ticketNumber}"
      }
    },
    "checkout": {
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/checkout"
      }
    },
    "addCart": {
      "fields": {
        "itemsMap": {
          "env" : "ITEMS_MAP"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/cart"
      }
    },
    "clearCart": {
      "fields": {
        "cartItemId": {
          "env": "CART_ITEM_ID"
        }
      },
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/cart/{cart_item_id}"
      }
    },
    "lookupCart": {
      "endpoint": {
        "href": "${CONNECTOR_HOST}/api/v1/cart/"
      }
    }
  }
}